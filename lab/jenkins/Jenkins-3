pipeline {
    agent any

    stages {
        stage('Copy git repo') {
            steps {
                sh "ls -lsa"
            }
        }
        stage('Display readme') {
            steps {
                //echo "fin"
                sh 'cat README.md'
            }
        }

        stage('Stage-git') {
            steps {
                 echo "${GIT_COMMIT}"
            }
        }
        stage('Stage-git') {
            steps {
                 echo "${BUILD_NUMBER}"
            }
        }
    }
}