pipeline {
    agent any

    stages {
        stage('Ejecutar Docker') {
            steps {
                script {
                    // Ejecutar comando docker version
                    sh "docker version"
                }
            }
        }
        
        stage('Ejecutar Kubernetes') {
            steps {
                script {
                    // Ejecutar comando kubectl version
                    sh "kubectl version"
                }
            }
        }
    }
}               
